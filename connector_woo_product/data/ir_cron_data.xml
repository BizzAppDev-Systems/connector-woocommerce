            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True" />
            <field name="active" eval="False" />
        </record>
        <!-- Import Product Attribute Value-->
        <record id="ir_cron_woocommerce_import_product_attribute_value" model="ir.cron">
            <field name="name">Woocommerce: Import Product Attribute Values</field>
            <field name="model_id" ref="model_woo_backend" />
            <field name="state">code</field>
            <field name="code">model.cron_import_product_attributes_value()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True" />
            <field name="active" eval="False" />
        </record>
        <!-- Import Product Categories -->
        <record id="ir_cron_woocommerce_import_product_category" model="ir.cron">
            <field name="name">Woocommerce: Import Product Categories</field>
            <field name="model_id" ref="model_woo_backend" />
            <field name="state">code</field>
            <field name="code">model.cron_import_product_categories()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True" />
            <field name="active" eval="False" />
        </record>
    </data>
</odoo>
