<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Create channel for the Woocommerce -->
        <record id="channel_woo" model="queue.job.channel">
            <field name="name">woo</field>
            <field name="parent_id" ref="queue_job.channel_root" />
        </record>
        <!-- Import Batch related action queue job -->
        <record
            id="job_function_woo_import_res_partner_record"
            model="queue.job.function"
        >
            <field
                name="model_id"
                ref="connector_woo_base.model_woo_res_partner"
            />
            <field name="method">import_record</field>
            <field
                name="related_action"
                eval='{"func_name": "related_action_open_view"}'
            />
            <field name="channel_id" ref="channel_woo" />
        </record>
        <!-- Import Invoices related action queue job -->
        <record
            id="job_function_woo_import_res_partner_batch"
            model="queue.job.function"
        >
            <field
                name="model_id"
                ref="connector_woo_base.model_woo_res_partner"
            />
            <field name="method">import_batch</field>
            <field name="channel_id" ref="channel_woo" />
        </record>
        <!-- Export Batch related action queue job -->
        <record
            id="job_function_woo_res_partner_export_batch"
            model="queue.job.function"
        >
            <field
                name="model_id"
                ref="connector_woo_base.channel_woo"
            />
            <field name="method">export_batch</field>
            <field name="channel_id" ref="channel_woo" />
        </record>

        <!-- Export Partners related action queue job-->
        <record
            id="job_function_woo_export_record"
            model="queue.job.function"
        >
            <field
                name="model_id"
                ref="connector_woo_base.channel_woo"
            />
            <field name="method">export_record</field>
            <field
                name="related_action"
                eval='{"func_name": "related_action_open_view"}'
            />
            <field name="channel_id" ref="channel_woo" />
        </record>
    </data>
</odoo>
